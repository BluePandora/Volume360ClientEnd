   <children>
      <HBox layoutY="-1.0" prefHeight="57.0" prefWidth="225.0" spacing="20.0" styleClass="list-cell-holder" stylesheets="@../styles/SlideMenuStyle.css">
         <children>
            <ImageView fx:id="slidingListIcon" fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/ic_ongoing_24dp.png" />
               </image>
            </ImageView>
            <Label fx:id="itemText" prefHeight="36.0" stylesheets="@../styles/SlideMenuStyle.css" text="Done">
               <font>
                  <Font name="Roboto2000980 Light" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Pane fx:id="dividerPane" layoutY="65.0" prefHeight="1.0" prefWidth="225.0" style="-fx-background-color: #AAAAAA;" />
   </children>
</AnchorPane>
