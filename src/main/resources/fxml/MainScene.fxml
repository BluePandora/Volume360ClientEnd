<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.collections.FXCollections ?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" styleClass="background" stylesheets="@../styles/MainSceneStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.volume360.LogInSceneController">
   <children>
      <ScrollPane layoutY="60.0" prefHeight="660.0" prefWidth="250.0" styleClass="slide-menu">
        <content>
          <AnchorPane styleClass="slide-menu-front">
               <children>
                  <Pane prefHeight="658.0" prefWidth="248.0" styleClass="slide-menu-front" />
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <ListView layoutX="250.0" layoutY="80.0" prefHeight="380.0" prefWidth="910.0">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="Book ID" />
               <String fx:value="Book Name" />
               <String fx:value="Author" />
               <String fx:value="Publisher" />
               <String fx:value="ISBN" />
               <String fx:value="Shelf Number" />
               <String fx:value="All" />
            </FXCollections>
         </items>
      </ListView>
      <Pane prefHeight="60.0" prefWidth="1280.0" styleClass="action-bar">
         <children>
            <ImageView fx:id="menuButton" fitHeight="40.0" fitWidth="40.0" layoutX="14.0" layoutY="10.0" onMouseClicked="#onMenuPressed" opacity="0.9" pickOnBounds="true" preserveRatio="true" styleClass="menu-button">
               <image>
               <Image url="@../styles/images/ic_menu_white_24dp.png" />
               </image>
            </ImageView>
            <ImageView fx:id="imageView" fitHeight="50.0" fitWidth="50.0" layoutX="1197.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../styles/images/9397052.jpg" />
               </image>
            </ImageView>
            <TextField fx:id="searchField" layoutX="272.0" layoutY="10.0" prefHeight="40.0" prefWidth="860.0" promptText="Search" styleClass="search-field">
               <font>
                  <Font name="Roboto Bold" size="18.0" />
               </font>
            </TextField>
            <ImageView fx:id="searchImage" fitHeight="36.0" fitWidth="36.0" layoutX="278.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/ic_search_white_18dp.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane layoutX="1155.0" layoutY="60.0" prefHeight="660.0" prefWidth="125.0">
         <children>
            <ImageView fitHeight="75.0" fitWidth="75.0" layoutX="14.0" layoutY="525.0" pickOnBounds="true" preserveRatio="true" styleClass="image-button">
               <image>
                  <Image url="@../styles/images/fab_add_new_project.default.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </children>
</AnchorPane>
